  "bugs": {
    "url": "https://github.com/allenjwelch/meal_planner/issues"
  },
  "homepage": "https://github.com/allenjwelch/meal_planner#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.10"
  }
}
